{	
	"manifest_version":2,
	"name":"myfirstextension",
 	"version": "1.0",
  	"description":" disable youtube extensions",
  	"icons":{
  		"16":"icon.png",
  		"48":"icon.png",
  		"128":"icon.png"
  		},
	


		


  		"permissions": [
			"tabs",
    			"https://www.youtube.com/*" 
    			
  				],


  		"browser_action": {
     	 "default_title": "click to alert.",
     	 "default_icon": "icon.png",
     	 "default_popup": "popup.html"
  		}


  		

 }
